
@{
    ViewData["Title"] = "Filtro de Pesquisa";
}


<h3 style="padding-top: 50px">@ViewData["Title"]</h3>
<hr />

<div class="row">
    <div class="col-md-12 cmd12">
        <form asp-action="">
            <div class="filtroLeft form-group ">
                <label @*asp-for="Nome"*@ class="control-label">Wiki Pesquisa</label>
                <input @*asp-for="Nome"*@ class="form-control" placeholder="Tipo: Banco de Dados, Algoritmo, Outros " />
                <span @*asp-validation-for="Nome"*@ class="text-danger"></span>
            </div>

            <div class="filtroLeft form-group">

                <label @*asp-for="Nome"*@ class="control-label">Tipo de Pesquisa</label>
                <input @*asp-for="Nome"*@ class="form-control" placeholder="Tipo: Banco de Dados, Algoritmo, Outros " />
                <span @*asp-validation-for="Nome"*@ class="text-danger"></span>
            </div>

            <div class="filtroLeft form-group">
                <label @*asp-for="Nome"*@ class="control-label">Categoria</label>
                <input @*asp-for="Nome"*@ class="form-control" placeholder="Tipo: Banco de Dados, Algoritmo, Outros " />
                <span @*asp-validation-for="Nome"*@ class="text-danger"></span>
            </div>

            <div class="filtroLeft form-group">
                <label @*asp-for="Nome"*@ class="control-label">Usuário</label>
                <input @*asp-for="Nome"*@ class="form-control" placeholder="Tipo: Banco de Dados, Algoritmo, Outros " />
                <span @*asp-validation-for="Nome"*@ class="text-danger"></span>
            </div>


        </form>
    </div>
</div>

